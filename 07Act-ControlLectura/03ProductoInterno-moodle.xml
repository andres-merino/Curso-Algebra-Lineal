  <answernumbering>abc</answernumbering>
  <answer fraction="100" format="html">
    <text><![CDATA[<p>El cuadrado de una función real no es negativo.</p>]]></text>
  </answer>
  <answer fraction="0" format="html">
    <text><![CDATA[<p>La integral de una función siempre es positiva.</p>]]></text>
  </answer>
  <answer fraction="0" format="html">
    <text><![CDATA[<p>La integral de una función es independiente de sus límites.</p>]]></text>
  </answer>
  <answer fraction="0" format="html">
